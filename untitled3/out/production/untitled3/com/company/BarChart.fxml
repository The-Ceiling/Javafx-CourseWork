<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane fx:id="Gridpane" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerBarChart">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <BarChart fx:id="FrequencyBarChart" title="Frequency Of Rolls">
            <xAxis>
                <CategoryAxis fx:id="X" label="Number" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="Y" label="Frequency" side="LEFT" />
            </yAxis>
        </BarChart>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" />
    </children>
</GridPane>
